{
  "name": "geom-api-ts-client",
  "version": "0.0.0-development",
  "description": "A TypeScript client library for accessing GeOM APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "npm link && nodemon",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nexioinformatica/geom-api-ts-client.git"
  },
  "keywords": [
    "GeOM",
    "APIs",
    "Client",
    "TypeScript"
  ],
  "author": {
    "name": "Nexio Informatica Srl",
    "email": "info@nexioinformatica.com",
    "url": "https://www.nexioinformatica.com"
  },
  "contributors": [
    {
      "name": "Luca Parolari",
      "email": "luca.parolari@nexio.cloud"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nexioinformatica/geom-api-ts-client/issues"
  },
  "homepage": "https://github.com/nexioinformatica/geom-api-ts-client#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn lint && npm test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@types/jest": "26.0.9",
    "@types/node": "12.12.20",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "26.4.0",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "semantic-release": "17.1.1",
    "ts-jest": "26.2.0",
    "typescript": "3.9.7"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {}
}
